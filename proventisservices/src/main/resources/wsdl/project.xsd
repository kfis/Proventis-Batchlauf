               </xsd:element>
               <xsd:element name="wtCalendarID" type="base:id">
                  <xsd:annotation>
                     <xsd:documentation>PrimaryKey des Arbeitszeit-Kalenders</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>

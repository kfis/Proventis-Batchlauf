   </xsd:element>
   <xsd:element name="getProjectListRequestParameter">
      <xsd:complexType>

         <xsd:sequence>
            <xsd:element name="sessionID" type="xsd:string"/>
            <xsd:element name="projectName" nillable="true" type="xsd:string">
               <xsd:annotation>
                  <xsd:documentation>
							Nur Projekte mit diesem Projektnamen holen
							(Like-Suche)
						</xsd:documentation>
               </xsd:annotation>
            </xsd:element>

            <xsd:element name="projectNumber" nillable="true" type="xsd:string">
               <xsd:annotation>
                  <xsd:documentation>
							Nur Projekte mit dieser ProjektNummer holen
							(Like-Suche)
						</xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="projectID" nillable="true" type="base:id">
               <xsd:annotation>

                  <xsd:documentation>
							Nur Projekte holen, die zu dieser ProjektID
							holen
						</xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="fromDate" nillable="true" type="xsd:date">
               <xsd:annotation>
                  <xsd:documentation>
							Nur Projekte holen, die nach fromDate liegen
						</xsd:documentation>

               </xsd:annotation>
            </xsd:element>
            <xsd:element name="toDate" nillable="true" type="xsd:date">
               <xsd:annotation>
                  <xsd:documentation>
							Nur Projekte holen, die vor toDate liegen
						</xsd:documentation>
               </xsd:annotation>
            </xsd:element>

            <xsd:element name="ownProjectsOnly" nillable="true" type="base:optionalBoolean">
               <xsd:annotation>
                  <xsd:documentation>
							Nur eigene Projekte holen
						</xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="offerNumber" nillable="true" type="xsd:string">
               <xsd:annotation>

                  <xsd:documentation>
							Nur Projekte holen, zu denen diese
							Angebotsnummer geh철rt (Like-Suche)
						</xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="ticketsOnly" nillable="true" type="base:optionalBoolean">
               <xsd:annotation>
                  <xsd:documentation>Nur Projekte holen, bei denen Tickets aktiviert sind.</xsd:documentation>
               </xsd:annotation>

            </xsd:element>
         </xsd:sequence>
      </xsd:complexType>
   </xsd:element>
   <xsd:element name="Project" type="project:T_Project"/>
   <xsd:complexType name="T_Project">
      <xsd:complexContent>
         <xsd:extension base="cost:T_SimpleProject">
            <xsd:sequence>

               <xsd:element name="isVirtuell" type="xsd:boolean">
                  <xsd:annotation>
                     <xsd:documentation>Projektvorlage (virtuell = true) / Projekt</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="projectNumber" type="xsd:string">
                  <xsd:annotation>
                     <xsd:documentation>ProjektNummer</xsd:documentation>

                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="projectTypeID" type="base:id">
                  <xsd:annotation>
                     <xsd:documentation>PrimaryKey des ProjektTypes (siehe Stammdaten)</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="creatorID" nillable="true" type="base:id">

                  <xsd:annotation>
                     <xsd:documentation>Referenz auf den Ersteller des Projektes (siehe Personenliste) - nur lesend</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="priorityID" type="base:id">
                  <xsd:annotation>
                     <xsd:documentation>Referenz auf die Priorit채t des Projektes (siehe Stammdaten)</xsd:documentation>
                  </xsd:annotation>

               </xsd:element>
               <xsd:element name="currencyID" type="base:id">
                  <xsd:annotation>
                     <xsd:documentation>Referenz auf die W채hrung des Projektes (siehe Stammdaten)</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="stateID" type="base:id">
                  <xsd:annotation>

                     <xsd:documentation>Referenz auf den Status des Projektes (siehe Stammdaten)</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="stateDate" nillable="true" type="xsd:date">
                  <xsd:annotation>
                     <xsd:documentation>Wann wurde der Status gesetzt ? - nur lesend</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>

               <xsd:element name="costUnitNo" nillable="true" type="xsd:string">
                  <xsd:annotation>
                     <xsd:documentation>Kostenstelle</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="projectLeaderID" type="base:id">
                  <xsd:annotation>
                     <xsd:documentation>Referenz auf den Projektleiter (siehe Personenliste)</xsd:documentation>

                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="worktimeTaskRequired" nillable="true" type="xsd:boolean">
                  <xsd:annotation>
                     <xsd:documentation>Aktivit채t als Pflichtfeld f체r die Arbeitszeiterfassung - nur lesend</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="startdateForTask" nillable="true" type="xsd:int">

                  <xsd:annotation>
                     <xsd:documentation>Startdatum neuer Aktivit채ten (0=Projektstart, 1=Aktuelles Datum) - nur lesend - wird bei Anlage/?nderung ignoriert</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="departmentID" type="base:id">
                  <xsd:annotation>
                     <xsd:documentation>Referenz auf den Unternehmensbereich dem das Projekt zugeordnet ist</xsd:documentation>
                  </xsd:annotation>

               </xsd:element>
               <xsd:element name="projectLeaderRoleID" nillable="true" type="base:id">
                  <xsd:annotation>
                     <xsd:documentation>Referenz auf die Rolle des Projektleiter - nur Anlage</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="projectLeaderParticipation" nillable="true" type="base:optionalDouble">
                  <xsd:annotation>

                     <xsd:documentation>Beteiligung des Projektleiters in % - nur Anlage - wird bei ?nderung ignoriert</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="isObligatory" type="xsd:boolean">
                  <xsd:annotation>
                     <xsd:documentation>Aktivierungspflichtig</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>

               <xsd:element name="costObjectNumber" nillable="true" type="xsd:string">
                  <xsd:annotation>
                     <xsd:documentation>Kostentr채ger</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="customers" type="project:T_CustomerList">
                  <xsd:annotation>
                     <xsd:documentation>Liste der Auftraggeber</xsd:documentation>

                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:element name="ProjectList">
      <xsd:complexType>
         <xsd:sequence>

            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="project:Project"/>
         </xsd:sequence>
      </xsd:complexType>
   </xsd:element>
   <xsd:element name="Customer" type="project:T_Customer"/>
   <xsd:complexType name="T_Customer">
      <xsd:sequence>
         <xsd:element name="customerID" nillable="true" type="base:id">
            <xsd:annotation>

               <xsd:documentation>Referenz auf den Auftraggber - von masterdata:getDepartments</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="share" type="xsd:double">
            <xsd:annotation>
               <xsd:documentation>Verrechnungssatz in %</xsd:documentation>
            </xsd:annotation>
         </xsd:element>

      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="T_CustomerList">
      <xsd:sequence>
         <xsd:element maxOccurs="unbounded" minOccurs="0" ref="project:Customer"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType abstract="true" name="T_AbstractResourceHead">
      <xsd:sequence>

         <xsd:element name="resourceID" nillable="true" type="base:id">
            <xsd:annotation>
               <xsd:documentation>PrimaryKey der Ressource.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType abstract="true" name="AbstractResource">

      <xsd:complexContent>
         <xsd:extension base="project:T_AbstractResourceHead">
            <xsd:sequence>
               <xsd:element name="roleID" type="base:id">
                  <xsd:annotation>
                     <xsd:documentation>PrimaryKey der Projektrolle</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>

               <xsd:element name="hasIndividualCosts" type="xsd:boolean">

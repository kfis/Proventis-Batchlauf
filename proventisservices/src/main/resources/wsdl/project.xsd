      <xsd:complexContent>
         <xsd:extension base="cost:T_SimpleProject"/>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:element name="ParticipationProjectList">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="project:ParticipationProject"/>
         </xsd:sequence>


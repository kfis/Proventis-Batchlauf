      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="sessionID" type="xsd:string"/>
            <xsd:element name="projectID" type="base:id">
               <xsd:annotation>
                  <xsd:documentation>F체r welches Projekt wird die Mitgliedschaft beantragt ?</xsd:documentation>
               </xsd:annotation>

            </xsd:element>
            <xsd:element name="taskID" nillable="true" type="base:id">
               <xsd:annotation>
                  <xsd:documentation>F체r welche Aktivit채t wird die Mitgliedschaft beantragt ?</xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="message" nillable="true" type="xsd:string">
               <xsd:annotation>

                  <xsd:documentation>Nachricht an den Projektleiter, die der Mitarbeitererfassen kann.</xsd:documentation>

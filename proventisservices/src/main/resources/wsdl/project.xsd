   <xsd:element name="ProjectParticipation" type="project:T_ProjectParticipation"/>
   <xsd:complexType name="T_ProjectParticipation">
      <xsd:sequence>
         <xsd:element name="projectParticipationID" nillable="true" type="base:id">
            <xsd:annotation>
               <xsd:documentation>PrimaryKey der Projektbeteiligung - NULL bei Neuanlage</xsd:documentation>
            </xsd:annotation>
         </xsd:element>

         <xsd:element name="resourceID" type="base:id">
            <xsd:annotation>
               <xsd:documentation>PrimaryKey der Ressource</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="from" type="xsd:date">
            <xsd:annotation>
               <xsd:documentation>Beginn des Beteiligungszeitraumes</xsd:documentation>

            </xsd:annotation>
         </xsd:element>
         <xsd:element name="to" type="xsd:date">
            <xsd:annotation>
               <xsd:documentation>Ende des Beteiligungszeitraumes</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="participation_in_percent" nillable="true" type="xsd:double">


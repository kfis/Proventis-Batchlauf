               </xsd:element>
               <xsd:element name="externalCost" nillable="true" type="base:optionalDouble">
                  <xsd:annotation>
                     <xsd:documentation>Externer Kostensatz</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="travelCost" nillable="true" type="base:optionalDouble">


   <xsd:simpleType name="WorkUnitType">
      <xsd:restriction base="xsd:string">
         <xsd:enumeration value="work_in_pt">
            <xsd:annotation>

               <xsd:documentation>Arbeit in Personentagen</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="work_in_h">
            <xsd:annotation>
               <xsd:documentation>Arbeit in Stunden</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>

      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="ProjectParticipationState">
      <xsd:restriction base="xsd:string">
         <xsd:enumeration value="requested">
            <xsd:annotation>
               <xsd:documentation>Die Projektbeteiligung ist angefragt.</xsd:documentation>
            </xsd:annotation>

         </xsd:enumeration>
         <xsd:enumeration value="released">
            <xsd:annotation>
               <xsd:documentation>Die Projektbeteiligung wurde genehmigt.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="refused">
            <xsd:annotation>

               <xsd:documentation>Die Projektbeteiligung wurde abgelehnt.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>

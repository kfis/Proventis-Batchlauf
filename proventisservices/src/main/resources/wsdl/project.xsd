            <xsd:annotation>
               <xsd:documentation>Erfassung erfolgte in Prozent.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="work">
            <xsd:annotation>
               <xsd:documentation>Erfassung erfolgte als Arbeit.</xsd:documentation>
            </xsd:annotation>

         </xsd:enumeration>
         <xsd:enumeration value="hours_weekly">
            <xsd:annotation>
               <xsd:documentation>Erfassung erfolgte in Wochenarbeitzeit.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>

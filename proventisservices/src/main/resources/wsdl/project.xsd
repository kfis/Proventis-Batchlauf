               <xsd:documentation>Liste der Projektbeteiligungen</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:element name="ProjectResourceList" type="project:T_ProjectResourceList"/>

   <xsd:complexType name="T_ProjectResourceList">
      <xsd:sequence>
         <xsd:element maxOccurs="unbounded" minOccurs="0" ref="project:ProjectResource"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:simpleType name="ProjectResourceReleaseType">
      <xsd:restriction base="xsd:string">
         <xsd:enumeration value="released">
            <xsd:annotation>

               <xsd:documentation>Alle Ressourcen, bei denen wenigstens eine Projektbeteiligung freigegeben ist.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="all">
            <xsd:annotation>
               <xsd:documentation>Alle Ressourcen, unabh채ngig von der Projektbeteiligung</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>

      </xsd:restriction>
   </xsd:simpleType>
   <xsd:element name="getProjectResourcesRequestParameter">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="sessionID" type="xsd:string"/>
            <xsd:element name="projectID" type="base:id">
               <xsd:annotation>
                  <xsd:documentation>Das gew체nschte Projekt</xsd:documentation>

               </xsd:annotation>
            </xsd:element>
            <xsd:element name="releaseType" type="project:ProjectResourceReleaseType">
               <xsd:annotation>
                  <xsd:documentation>Nur Ressourcen mit diesem FreigabeStatus liefern</xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="personID" nillable="true" type="base:id">

               <xsd:annotation>
                  <xsd:documentation>Nur Informationen 체ber diesen Mitarbeiter liefern</xsd:documentation>
               </xsd:annotation>
            </xsd:element>
         </xsd:sequence>
      </xsd:complexType>
   </xsd:element>
   <xsd:element name="getProjectRolesRequestParameter">

      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="sessionID" type="xsd:string"/>
            <xsd:element name="projectID" type="base:id"/>
         </xsd:sequence>
      </xsd:complexType>
   </xsd:element>
   <xsd:element name="ProjectRole" type="project:T_ProjectRole"/>
   <xsd:complexType name="T_ProjectRole">

      <xsd:sequence>
         <xsd:element name="projectRoleID" type="base:id">
            <xsd:annotation>
               <xsd:documentation>PrimaryKey</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="name" type="xsd:string">
            <xsd:annotation>

               <xsd:documentation>Bezeichnung der Projektrolle</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="internal" type="xsd:double">
            <xsd:annotation>
               <xsd:documentation>Interner Kostensatz</xsd:documentation>
            </xsd:annotation>
         </xsd:element>

         <xsd:element name="external" type="xsd:double">
            <xsd:annotation>
               <xsd:documentation>Externer Kostensatz</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="travel" type="xsd:double">
            <xsd:annotation>
               <xsd:documentation>Kostensatz f체r Reiset채tigkeiten</xsd:documentation>

            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:element name="ProjectRoleList">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element maxOccurs="unbounded" ref="project:ProjectRole"/>
         </xsd:sequence>

      </xsd:complexType>
   </xsd:element>
   <xsd:simpleType name="ProjectPermissionType">
      <xsd:restriction base="xsd:string">
         <xsd:enumeration value="leader">
            <xsd:annotation>
               <xsd:documentation>Projektleiter</xsd:documentation>
            </xsd:annotation>

         </xsd:enumeration>
         <xsd:enumeration value="substitute">
            <xsd:annotation>
               <xsd:documentation>Stellvertreter</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="member">
            <xsd:annotation>

               <xsd:documentation>Mitarbeiter</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="extern">
            <xsd:annotation>
               <xsd:documentation>externer Mitarbeiter</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>

      </xsd:restriction>
   </xsd:simpleType>
   <xsd:element name="ProjectPermission" type="project:T_ProjectPermission"/>
   <xsd:complexType name="T_ProjectPermission">
      <xsd:sequence>
         <xsd:element name="projectPermissionID" type="base:id">
            <xsd:annotation>
               <xsd:documentation>PrimaryKey</xsd:documentation>

            </xsd:annotation>
         </xsd:element>
         <xsd:element name="name" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Bezeichnung der Projektrolle</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="typ" type="project:ProjectPermissionType">


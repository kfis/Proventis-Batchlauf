<?xml version="1.0" encoding="ISO-8859-1"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:project="http://project.blueant.axis.proventis.net/" xmlns:base="http://base.blueant.axis.proventis.net/" xmlns:masterdata="http://masterdata.blueant.axis.proventis.net/" xmlns:cost="http://cost.blueant.axis.proventis.net/" xmlns:human="http://human.blueant.axis.proventis.net/" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://project.blueant.axis.proventis.net/">
   <xsd:import namespace="http://base.blueant.axis.proventis.net/" schemaLocation="base.xsd"/>
   <xsd:import namespace="http://masterdata.blueant.axis.proventis.net/" schemaLocation="masterdata.xsd"/>
   <xsd:import namespace="http://human.blueant.axis.proventis.net/" schemaLocation="human.xsd"/>
   <xsd:import namespace="http://cost.blueant.axis.proventis.net/" schemaLocation="cost.xsd"/>
   <xsd:import namespace="http://absence.blueant.axis.proventis.net/" schemaLocation="absence.xsd"/>
   <xsd:import namespace="http://ticket.blueant.axis.proventis.net/" schemaLocation="ticket.xsd"/>
   <xsd:element name="ParticipationProject" type="project:T_ParticipationProject"/>
